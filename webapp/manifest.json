{
  "_version": "1.48.0",
  "sap.app": {
    "id": "com.borderstates.topazsignature",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "embeds": ["lib"],
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.8.3",
      "toolsId": "2a0316ee-6e02-4052-b2c8-c6fc8f5642a8"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZTOPAZ_INTEGRATION_API/",
        "type": "OData",
        "settings": {
          "localUri": "/localService/ZTOPAZ_INTEGRATION_API/metadata.xml "
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.112.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.borderstates.topazsignature.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.borderstates.topazsignature.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteSignatureCapture",
          "pattern": ":?query:",
          "target": ["TargetSignatureCapture"]
        }
      ],
      "targets": {
        "TargetSignatureCapture": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "SignatureCapture",
          "viewName": "SignatureCapture"
        }
      }
    },
    "rootView": {
      "viewName": "com.borderstates.topazsignature.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
